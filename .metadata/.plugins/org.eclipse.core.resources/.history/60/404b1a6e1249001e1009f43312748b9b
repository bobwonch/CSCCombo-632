public class HelloWorld {

	public static void main(String[] args) {
		//Hello world one line comment
		System.out.println("Hello World!");
		
		System.out.println();
		
		int num1 = 5;
		
		double num2 = 6.0;
		
		int total = (int) (num1 / num2);
		
		System.out.println("The sum of the two numbers is " + total);
	}

}
